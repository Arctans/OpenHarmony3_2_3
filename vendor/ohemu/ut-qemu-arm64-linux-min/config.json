{
    "product_name": "ut-qemu-arm64-linux-min",
    "type": "standard",
    "version": "3.0",
    "device_company": "qemu",
    "board": "ut-qemu-arm64-linux",
    "target_cpu": "arm64",
    "target_os": "ohos",
    "enable_ramdisk": true,
    "support_jsapi": false,
    "device_build_path": "device/qemu/arm_virt/linux",
    "subsystems": [
      {
        "subsystem": "common",
        "components": [
          { "component": "common", "features":[] }
        ]
      },
     {
        "subsystem": "hdf",
         "components": [
          { "component": "hdf_core", "features":[] },
          {
            "component": "drivers_peripheral_light",
            "features": []
          },
          {
            "component": "drivers_peripheral_input",
            "features": [
            "drivers_peripheral_input_feature_model = true"
            ]
          },
          {
			"component": "drivers_peripheral_battery",
			"features": []
		  }
        ]
      },
      {
        "subsystem": "startup",
        "components": [
          { "component": "startup_l2", "features":[] },
          { "component": "init", "features":[] }
        ]
      },
      {
        "subsystem": "hiviewdfx",
        "components": [
          { "component": "hiviewdfx_hilog_native", "features":[] },
          { "component": "hilog_native", "features":[] },
          { "component": "hilog_service", "features":[] },
          { "component": "hitrace_native", "features":[] },
          { "component": "faultloggerd", "features":[] },
          { "component": "hisysevent_native", "features":[] },
          { "component": "hichecker_native", "features":[] }
        ]
      },
      {
        "subsystem": "distributedhardware",
        "components": [
          { "component": "device_manager", "features": [] }
        ]
      },
      {
        "subsystem": "security",
        "components": [
          { "component": "device_auth", "features":[] },
          { "component": "access_token", "features":[] },
          { "component": "huks", "features": [] }
        ]
      },
      {
        "subsystem": "commonlibrary",
        "components": [
          { "component": "c_utils", "features": [] }
        ]
      },
      {
        "subsystem": "communication",
        "components": [
          { "component": "ipc", "features":[] },
          { "component": "dsoftbus", "features":["dsoftbus_get_devicename=false"] }
        ]
      },
      {
        "subsystem": "notification",
        "components": [
          { "component": "eventhandler", "features":[] }
        ]
      },
      {
        "subsystem": "systemabilitymgr",
        "components": [
          { "component": "samgr", "features":[] },
          { "component": "safwk", "features":[] }
        ]
      },
      {
        "subsystem": "developtools",
        "components": [
          { "component": "helloworld", "features":[] },
          { "component": "bytrace", "features":[] },
          { "component": "hdc", "features":[] }
        ]
      },
	  {
        "subsystem": "securec",
        "components": [
          { "component": "thirdparty_bounds_checking_function", "features":[] }
        ]
      },
      {
        "subsystem": "device_arm_virt",
        "components": [
          { "component": "qemu_arm_linux_chipset", "features":[] }
        ]
      }
    ]
  }
